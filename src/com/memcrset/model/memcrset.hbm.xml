<hibernate-mapping>
	<class name="com.memcrset.model.MemCRSetVO" table="MEMCRSET">
		
		<id name="memCRSet_no" column="MEMCRSET_NO">
			<generator class="sequence">
				<param name="sequence">SEQ_MEMCRSETNO</param>
